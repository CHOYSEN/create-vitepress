{
  "name": "create-vitepress",
  "version": "0.0.4",
  "main": "./dist/index.mjs",
  "description": "Scaffolding Your Vitepress project.",
  "bin": {
    "create-vitepress": "./dist/index.mjs"
  },
  "scripts": {
    "test": "vitest run",
    "build": "unbuild",
    "format": "prettier --ignore-unknown --ignore-path .gitignore --write ."
  },
  "keywords": [
    "create-vitepress",
    "vitepress",
    "vite"
  ],
  "author": "CHOYSEN@qq.com",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "templates"
  ],
  "bugs": "https://github.com/CHOYSEN/create-vitepress/issues",
  "homepage": "https://github.com/CHOYSEN/create-vitepress#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/CHOYSEN/create-vitepress.git"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@types/prompts": "2.0.14",
    "execa": "6.1.0",
    "lint-staged": "13.0.3",
    "mri": "1.2.0",
    "picocolors": "1.0.0",
    "prettier": "2.7.1",
    "prompts": "2.4.2",
    "simple-git-hooks": "2.8.0",
    "tsx": "3.9.0",
    "unbuild": "0.8.9",
    "vitest": "0.22.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
